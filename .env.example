# KME Server Configuration
HOST=0.0.0.0
PORT=8090

# KME Identity
KME_ID=kme-id-uuidv4
ATTACHED_SAE_ID=sae-id-uuidv4

# Key Generation Settings
DEFAULT_KEY_SIZE=32
MAX_KEY_COUNT=100000
MAX_KEYS_PER_REQUEST=128
MAX_KEY_SIZE=1024
MIN_KEY_SIZE=32
KEY_GEN_SEC_TO_GEN=30
KEY_GEN_BATCH_SIZE=64
KEY_ACQUIRE_TIMEOUT=10
NETWORK_TIMEOUT=5

# Other KME nodes (comma-separated URLs)
OTHER_KMES=http://127.0.0.1:8091

# TLS/SSL Certificates
CA_FILE=ca.crt.pem
KME_CERT=kme.crt.pem
KME_KEY=kme.key.pem
SAE_CERT=sae.crt.pem

# MongoDB Configuration for persistent QKD key storage
# Set MONGODB_URI to enable MongoDB persistence for QKD key blocks
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=qumail_kme
